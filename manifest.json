{
    "name": "watch together",
    "version": "7.0",
    "description": "watch movies together in real time! created with <3 by raymond wang",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "inject.js"
    ],
    "permissions": [
        "storage"
    ]
}